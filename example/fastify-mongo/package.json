{
  "name": "example-fastify-mongo",
  "version": "0.1.0",
  "main": "src/index.ts",
  "description": "This is a test using Fastify with Prisma and MongoDB",
  "scripts": {
    "lint": "eslint src/* --ext .ts --fix",
    "service": "nodemon",
    "start": "ts-node src/index.ts",
    "release": "standard-version",
    "test:ci": "jest --ci -i",
    "test:local": "NODE_ENV=local jest --ci -i --setupFiles dotenv/config"
  },
  "author": "AnthonyLzq <sluzquinosa@uni.pe>",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/swagger": "^8.8.0",
    "@fastify/swagger-ui": "^1.9.3",
    "@prisma/client": "^5.1.0",
    "debug": "^4.3.4",
    "fastify": "^4.21.0",
    "fastify-type-provider-zod": "^1.1.9",
    "http-errors": "^2.0.0",
    "mongodb": "^5.7.0",
    "zod": "^3.21.4"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.test.ts"
    ],
    "exec": "DEBUG=App:* npx ts-node -r dotenv/config ./src/index"
  },
  "devDependencies": {
    "@jest/types": "^29.6.1",
    "@types/debug": "^4.1.8",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.6",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.2",
    "jest-unit": "^0.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "prisma": "^5.1.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
